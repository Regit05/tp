<%= link_to 'New Article', new_article_path %>
<%= link_to 'Contacts', contacts_path %>

<h1>Listing Article</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="3"></th>
  </tr>

  <% @admins.each do |admin| %>
    <tr>
      <td><%= admin.title %></td>
      <td><%= admin.text %></td>
      <td><%= link_to 'show', admin_path(admin) %></td>
      <td><%= link_to 'edit', edit_article_path(admin) %></td>
      <td><%= link_to 'Destroy', article_path(admin),
                      method: :delete,
                      data: { confirm: 'Are you sure?' } %></td>
  <% end %>

</table>